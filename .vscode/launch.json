{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Flutter App",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": ["--dart-define", "FLUTTER_WEB_USE_SKIA=false"]
        },
        {
            "name": "Profile Flutter App", 
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main.dart"
        },
        {
            "name": "Release Flutter App",
            "request": "launch", 
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main.dart"
        }
    ]
}
